  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "runtimeExecutable": "yarn",
      "cwd": "${workspaceFolder}",
      "runtimeArgs": ["dev:debug"],
      "env": {
        "NODE_ENV": "test",
        "PORT": "4000",
        "SCRAPER_URL": "http://localhost:6800",
        "SCRAPER_PROJECT": "default",
        "SCRAPER_USERNAME": "1234",
        "SCRAPER_PASSWORD": "1234"
      }
    }
  ]
}
